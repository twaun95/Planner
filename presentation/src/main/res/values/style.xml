<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools" tools:ignore="SpUsage">
    <style name="BottomNavigationView" parent="@style/TextAppearance.AppCompat.Caption">
        <item name="android:textSize">10dp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="BottomNavigationView_Active" parent="BottomNavigationView">
        <item name="android:textSize">11dp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="BottomNavigationView_InActive" parent="BottomNavigationView">
        <item name="android:textSize">10dp</item>
    </style>

    <style name="dialog_common" parent="Theme.AppCompat.Dialog">
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

</resources>